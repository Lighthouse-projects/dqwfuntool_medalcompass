{
  "expo": {
    "name": "dqwfun-medalcompass",
    "slug": "dqwfun-medalcompass",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lighthouseprojects.medalcompass",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "メダルの位置を登録・表示するために位置情報を使用します。",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "メダルの位置を登録・表示するために位置情報を使用します。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-1024.png",
        "backgroundColor": "#FFC107"
      },
      "package": "com.lighthouseprojects.medalcompass",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "d7a88ec2-6616-481c-8444-a4fe2bd297c3"
      }
    }
  }
}
